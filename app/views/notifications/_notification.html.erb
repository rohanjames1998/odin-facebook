  <% case notification.notifiable_type %>
  <% when "Friendship" %>
    <%= render partial: "notifications/friend_req_notification", locals: {notification: notification }%>
  <% when "Like" %>
    <%= render partial: "notifications/like_notification", locals: {notification: notification } %>
  <% when "Comment" %>
    <%= render partial: "notifications/comment_notification", locals: { comment: notification.notifiable, notification: notification } %>
  <% end %>
